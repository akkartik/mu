      (lookup *_Program-functions *_Program-functions->payload)  # => eax
      (find-matching-function %eax *(ebp+8))  # => eax
      3d/compare-eax-and 0/imm32
      {
        74/jump-if-= break/disp8
        (check-mu-call *(ebp+8) %eax *(ebp+0xc) *(ebp+0x10) *(ebp+0x14))
