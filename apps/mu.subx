size-of:  # v: (handle var) -> result/eax: int
    # . prologue
    55/push-ebp
    89/<- %ebp 4/r32/esp
    # . save registers
    51/push-ecx
    # ecx = v->type
    8b/-> *(ebp+8) 1/r32/ecx
    8b/-> *(ecx+4) 1/r32/ecx  # Var-type
    # TODO: support non-atom type
    # TODO: support arrays
