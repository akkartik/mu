    8b/-> *(ebp+8) 0/r32/eax
    39/compare *eax 1/r32/ecx  # Atom-type
    0f 94/set-if-= %eax
    81 4/subop/and %eax 0xff/imm32
$is-simple-mu-type?:end:
    # . restore registers
    59/pop-to-ecx
    # . epilogue
    89/<- %esp 5/r32/ebp
