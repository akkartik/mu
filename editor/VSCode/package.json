{
    "name": "mu",
    "displayName": "Mu",
    "description": "Syntax Highlighting for the Mu language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mu",
            "aliases": ["Mu", "mu"],
            "extensions": [".mu"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mu",
            "scopeName": "source.mu",
            "path": "./syntaxes/Mu.tmLanguage"
        }]
    }
}